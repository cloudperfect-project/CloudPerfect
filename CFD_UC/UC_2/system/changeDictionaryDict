FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


    boundary
    {


        WB_inlet
        {
            type            patch;
	    physicalType    Inlet;
        }
        WB_outlet
        {
            type            patch;
	    physicalType    Outlet;
        }
    }

 



