FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    ".*"
    {
        type            nutkWallFunction;
        value           uniform 0;
    }

    "proc.*"
    {
        type            processor;
        value           uniform 0;
    }

    GEO_inlet
    {
        type            calculated;
        value           uniform 0;
    }

    GEO_outlet
    {
        type            calculated;
        value           uniform 0;
    }

    GEO_pressure_reference
    {
        type            calculated;
        value           uniform 0;
    }
}


